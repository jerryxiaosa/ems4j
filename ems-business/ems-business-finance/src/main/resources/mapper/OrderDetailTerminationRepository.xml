<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="info.zhihui.ems.business.finance.repository.order.OrderDetailTerminationRepository">
    
    <select id="selectByOrderSn" resultType="info.zhihui.ems.business.finance.entity.order.OrderDetailTerminationEntity">
        SELECT *
        FROM order_detail_termination
        WHERE order_sn = #{orderSn}
        LIMIT 1
    </select>
    
</mapper>